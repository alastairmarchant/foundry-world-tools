[flake8]
select = B,B9,C,D,DAR,E,F,N,RST,S,W
ignore = E203,E501,RST201,RST203,RST301,W503,B907
max-line-length = 80
# Change back to 10 after refactor
max-complexity = 16
docstring-convention = google
# Possibly remove D1 for test documentation
per-file-ignores = tests/*:S101,D1
rst-roles = class,const,func,meth,mod,ref
rst-directives = deprecated
ignore-names=prototypeToken,dataDir,setUp,tearDown,setUpClass,tearDownClass,setUpModule,tearDownModule,asyncSetUp,asyncTearDown,setUpTestData,failureException,longMessage,maxDiff
